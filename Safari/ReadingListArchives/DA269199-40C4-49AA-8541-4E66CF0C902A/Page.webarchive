a:hover.non-ul {
  background: transparent;
  color:#932100;
  text-decoration: underline;
}
a:active.non-ul {
  color:#005c9e;
  background: transparent;
  text-decoration: none;
}
.story-title {
  padding-left:5px;
  border-top:1px solid #bac0cd;
  border-bottom:1px solid #bac0cd;
  vertical-align:middle;
  background: #ffffff;
  color:#000033;
  font: bold 135% tahoma,helvetica,arial,sans-serif;
}
.story-title-featured {
  padding-left:5px;
  background: #ced4e3;
  color:#000033;
  font: bold 125% tahoma,helvetica,arial,sans-serif;
}
.story-control {
  line-height: 1.6 !important;
  color:#3c3c3c;
  font: 90% verdana,tahoma,helvetica,arial,sans-serif;
  background-color:#F1F6FA;
  border-bottom:1px solid #DEDEDE;
  border-top:1px solid #DEDEDE;
  margin-bottom:40px;
  margin-top:10px;
  padding-right:5px;
  padding-top:5px;
  padding-bottom:5px;
}
.story-control:link {
  color:#3c3c3c;
  text-decoration: underline;
}
.story-control :visited{
  color:#3c3c3c;
}
.story-control :hover{
  color:#932100;
}
pre code, tt code {
    color: #9F2D00;
    font-family: Monaco, Courier, monospace;
    font-size: 100%
}
tt, code, pre {
    color: #9F2D00;
    font-family: Monaco, Courier, monospace;
    font-size: 12px;
}
a:link {
  background:transparent;
  color:#0000FF;
}
a:visited {
  background:transparent;
  color:#800080
}
a:hover {
  background:transparent;
  color:#008000
}
a:active {
  background:transparent;
  color:#FF0000
}
.block-bg-right a:link {
  color: #000066;
  text-decoration: none; 
}

.block-bg-right a:hover {
  background-color: #fff3a2;
  background-image: none;
  background-repeat: repeat;
  background-attachment: scroll;
  background-position: 0% 0%;
  color: #932100;
}

.block-bg-right a:visited {
  text-decoration:none;
  color: #005f7a;
}
.block-bg-right a:visited:hover {
  background-color:transparent;
  border-bottom-width: 1px;
  border-bottom-style: dotted;
  border-bottom-color: #005f7a;
}
.xpinputfield {
  border:1px solid #7B9EBD;
  padding-top:2px;
  padding-bottom:1px;
  margin-bottom:1px;
}
.footerblock {
  border-top:1px solid #CCCCCC;
  color:#000033;
  font: 85% verdana,tahoma,helvetica,arial,sans-serif;
  padding-bottom:10px;
  padding-top:2px;
  padding-left: 4px;
  padding-right: 4px;
}
.footerblock a:link {
  background:transparent;
  color:#000033;
  font:100% verdana,tahoma,helvetica,arial,sans-serif;
  text-decoration:underline
}
.footerblock a:visited {
  color:#000033;
  text-decoration:underline
}
.footerblock a:hover {
  color:#008000;
  font:100% verdana,tahoma,helvetica,arial,sans-serif;
  text-decoration:underline
}
.footerblock a:active {
  color:#000033;
  font:100% verdana,tahoma,helvetica,arial,sans-serif;
  text-decoration:underline
}
#IDG {
  padding:0 10px 0 0;
}
#IDG p {
  display:inline;
}
#IDG form {
  display:inline;
}

#ntwrk {
  margin:0;
  padding:0 0 10px 0;
}
#ntwrk img {
  float:left;
  padding:0 6px 0 5px;
}
#ntwrk  ul {
  margin:0;
  padding:0;
}

#ntwrk  ul li {
  margin:0;
  padding:0;
  list-style:none;
  float:left;
}

#ntwrk  ul li a {
  margin:0;
  padding:0 6px 0 0;
  color:#000033;
  font-family:verdana,tahoma,helvetica,arial,sans-serif;
  font-size:100%;
  font-size-adjust:none;
  font-stretch:normal;
  font-style:normal;
  font-variant:normal;
  font-weight:normal;
  line-height:normal;
  text-decoration:underline;
}
#ntwrk  ul li a:hover {
  color:#008000;
}

.story-body a:link{
  color: #000099;
}
.story-body a:visited{
  color: #000033;
}
.story-body a:hover{
  color:#932100;
}
.comment-author {
  padding-left: 2px;
  color:#1e1e1e;
  font:11px verdana,tahoma,helvetica,arial,sans-serif
}
.comment-author :link{
  color:#1e1e1e;
  text-decoration: underline;
}
.comment-author :visited{
  color:#1e1e1e;
}
.comment-author :hover{
  color:#932100;
}
.comment-divider {
  background:#FFFFFF;
  border-bottom:1px solid #F7F7F7;
  border-top:3px double #CCCCCC;
  color:#000000;
  padding-top:2px;
}
.comment-box {
  border:1px dotted #BAC0CD;
  border-top:none;
  padding:5px;
  margin-bottom:10px;
}
.comment-footer {
  font-family: verdana,tahoma,helvetica,arial,sans-serif;
  font-style: normal;
  font-variant: normal;
  font-weight: normal;
  font-size: 11px;
}
.comment-footer :link {
  color: #1e1e1e;
}
.comment-footer :hover {
  color: #932100;
}


